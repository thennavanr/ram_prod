<!DOCTYPE html>
<html>
<head>
<title>Form</title>
<%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true %>
<script src="http://code.jquery.com/jquery-1.9.1.js"></script>
<script>

$(document).ready(function() {

  $('#mainPage').show();
  $('#whoWeAre').hide();
  $('#ourSauces').hide();
  $('#recipes').hide();
  $('#contact').hide();
  $('#navigation').click(function() {
   
   

  });
   $('#home-nav').click(function(){
      $('#mainPage').show();
      $('#whoWeAre').hide();
      $('#ourSauces').hide();
      $('#recipes').hide();
      $('#contact').hide();

  });
  $('#who-nav').click(function(){
    $('#whoWeAre').show();
    $('#mainPage').hide();
    $('#ourSauces').hide();
    $('#recipes').hide();
    $('#contact').hide();

  });
  $('#sauces-nav').click(function(){
    $('#ourSauces').show();
    $('#mainPage').hide();
    $('#whoWeAre').hide();
    $('#recipes').hide();
    $('#contact').hide();


  });
  $('#recipes-nav').click(function(){
    $('#recipes').show();
    $('#mainPage').hide();
    $('#whoWeAre').hide();
    $('#ourSauces').hide();
    $('#contact').hide();



  });
  $('#contact-nav').click(function(){
    $('#contact').show();
    $('#mainPage').hide();
    $('#whoWeAre').hide();
    $('#ourSauces').hide();
    $('#recipes').hide();

  });



});

</script>
</head>

<body style device="desktop" preview="public">
